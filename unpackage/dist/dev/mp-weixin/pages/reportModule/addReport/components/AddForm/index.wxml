<view class="{{['addForm cu-list menu',wrapClass]}}"><block wx:if="{{title}}"><view class="cu-bar bg-white"><view class="action"><text class="cuIcon-titles text-green"></text><text class="text-xl text-bold">{{title}}</text></view></view></block><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item',item.arrowRight?'arrow':'']}}"><view class="title">{{item.label}}</view><block wx:if="{{item.arrowRight}}"><view data-event-opts="{{[['tap',[['handleArrowRightClick',['$0',index],[[['dataList','',index]]]]]]]}}" bindtap="__e"><text style="color:#717171;font-size:30rpx;">{{item.value||item.placeholder}}</text></view></block><block wx:else><block wx:if="{{item.component==='picker'}}"><u-picker vue-id="{{'60a319d2-1-'+index}}" mode="{{item.type||'date'}}" value="{{item.value}}" range="{{item.range}}" rangeKey="{{item.rangeKey}}" index="{{index}}" data-event-opts="{{[['^handleChange',[['handleChange']]]]}}" bind:handleChange="__e" bind:__l="__l"></u-picker></block><block wx:else><u-input class="flex-sub text-right" vue-id="{{'60a319d2-2-'+index}}" value="{{item.value}}" type="{{item.type||'text'}}" disabled="{{item.disabled}}" placeholder="{{item.placeholder}}" placeholderClass="text-right" index="{{index}}" data-event-opts="{{[['^handleInput',[['handleInput']]]]}}" bind:handleInput="__e" bind:__l="__l"></u-input></block></block></view></block><slot name="footer"></slot></view>