<view class="u-upload"><view class="bg-white"><block wx:if="{{$slots.header}}"><slot name="header"></slot></block><block wx:else><view class="cu-bar"><view class="action">{{title}}</view><view class="action">{{''+imgList.length+'/'+imgLength}}</view></view></block></view><view class="cu-form-group"><view class="{{['grid','col-'+col,'grid-square','flex-sub']}}"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="bg-img"><image src="{{imgList[index]}}" mode="aspectFill"></image><view data-event-opts="{{[['tap',[['deleteImage',['$event','$0'],['customBottomTextArr.'+index+'']]]]]}}" class="cu-tag bg-red" bindtap="__e"><text class="cuIcon-close"></text></view></view></block></block><block wx:if="{{customChoose&&customChoose.length>0}}"><block><block wx:for="{{$root.l0}}" wx:for-item="obj" wx:for-index="index" wx:key="index"><block><block wx:if="{{!obj.g0}}"><block><view data-event-opts="{{[['tap',[['chooseImage',['$event','$0'],[[['customChoose','',index,'bottomText']]]]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"><text class="text-sm">{{obj.$orig.bottomText}}</text></text></view></block></block></block></block></block></block><block wx:if="{{(!customChoose||customChoose.length)===0&&imgList.length<imgLength}}"><block><view data-event-opts="{{[['tap',[['chooseImage',['$event','$0'],['bottomText']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"><text class="text-sm">{{bottomText}}</text></text></view></block></block></view></view></view>